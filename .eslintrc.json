{
  "extends": "@mate-academy/eslint-config",
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "env": {
    "jest": true,
    "es2022": true,
    "node": true
  },
  "rules": {
    "no-proto": 0,
    "max-len": ["error", { "code": 120 }],
    "no-console": "off"
  },
  "plugins": ["jest", "@typescript-eslint"]
} 